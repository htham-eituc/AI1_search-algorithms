{
  "problems": {
    "sphere": {
      "name": "Sphere",
      "dimensions": [10, 30, 50],
      "search_space": [-5.12, 5.12],
      "global_minimum": 0.0,
      "minimum_at": "origin",
      "tolerance": 1e-6
    },
    "rastrigin": {
      "name": "Rastrigin",
      "dimensions": [10, 30, 50],
      "search_space": [-5.12, 5.12],
      "global_minimum": 0.0,
      "minimum_at": "origin",
      "tolerance": 1e-4
    },
    "rosenbrock": {
      "name": "Rosenbrock",
      "dimensions": [10, 30, 50],
      "search_space": [-2.048, 2.048],
      "global_minimum": 0.0,
      "minimum_at": "ones",
      "tolerance": 1e-4
    }
  },

  "algorithms": {
    "ABC": {
      "name": "Artificial Bee Colony",
      "defaults": {
        "max_iterations": 1000,
        "population_size": 50,
        "limit": null,
        "runs": 30
      },
      "problem_overrides": {
        "sphere": {
          "population_size": 50,
          "max_iterations": 1000
        }
      }
    },

    "CS": {
      "name": "Cuckoo Search",
      "defaults": {
        "max_iterations": 1000,
        "population_size": 50,
        "pa": 0.25,
        "alpha": 0.01,
        "lambda_levy": 1.5,
        "runs": 30
      },
      "problem_overrides": {
        "rastrigin": {
          "population_size": 100,
          "max_iterations": 2000,
          "pa": 0.3
        }
      }
    },

    "FA": {
      "name": "Firefly Algorithm",
      "defaults": {
        "max_iterations": 1000,
        "population_size": 50,
        "beta0": 1.0,
        "gamma": 1.0,
        "alpha": 0.5,
        "alpha_decay": 0.97,
        "runs": 30
      },
      "problem_overrides": {
        "rosenbrock": {
          "population_size": 50,
          "max_iterations": 2000,
          "beta0": 1.0,
          "gamma": 1.0
        }
      }
    },

    "PSO": {
      "name": "Particle Swarm Optimization",
      "defaults": {
        "max_iterations": 1000,
        "population_size": 50,
        "w_max": 0.9,
        "w_min": 0.4,
        "c1": 2.0,
        "c2": 2.0,
        "v_clamp_ratio": 0.2,
        "runs": 30
      },
      "problem_overrides": {
        "sphere": {
          "population_size": 50,
          "max_iterations": 1000
        }
      }
    },

    "GA": {
      "name": "Genetic Algorithm",
      "defaults": {
        "max_iterations": 1000,
        "population_size": 50,
        "crossover_rate": 0.8,
        "mutation_rate": 0.1,
        "mutation_scale": 0.1,
        "tournament_size": 3,
        "runs": 30
      },
      "problem_overrides": {
        "rastrigin": {
          "crossover_rate": 0.8,
          "mutation_rate": 0.15,
          "mutation_scale": 0.15,
          "population_size": 100,
          "max_iterations": 2000
        }
      }
    },

    "DE": {
      "name": "Differential Evolution",
      "defaults": {
        "max_iterations": 1000,
        "population_size": 50,
        "mutation_factor": 0.8,
        "crossover_rate": 0.9,
        "strategy": "rand/1/bin",
        "runs": 30
      },
      "problem_overrides": {
        "sphere": {
          "mutation_factor": 0.6,
          "crossover_rate": 0.8,
          "population_size": 30
        }
      }
    },

    "SA": {
      "name": "Simulated Annealing",
      "defaults": {
        "max_iterations": 5000,
        "initial_temperature": 1000.0,
        "cooling_rate": 0.95,
        "cooling_schedule": "exponential",
        "min_temperature": 1e-8,
        "step_size": 0.1,
        "runs": 30
      },
      "problem_overrides": {
        "rosenbrock": {
          "initial_temperature": 1000.0,
          "cooling_rate": 0.95,
          "step_size": 0.1,
          "max_iterations": 8000
        }
      }
    },

    "GSA": {
      "name": "Gravitational Search Algorithm",
      "defaults": {
        "max_iterations": 1000,
        "population_size": 50,
        "gravitational_constant": 100.0,
        "alpha": 20,
        "epsilon": 1e-10,
        "runs": 30
      },
      "problem_overrides": {
        "sphere": {
          "gravitational_constant": 50.0,
          "population_size": 30
        }
      }
    }
  },

  "experiment": {
    "seed": 42,
    "dimensions": [10, 30, 50],
    "metrics": ["best_fitness", "mean_fitness", "std_fitness", "convergence_iteration"],
    "save_convergence_curve": true,
    "output_dir": "results/"
  }
}
